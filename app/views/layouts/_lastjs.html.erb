<!--! end of #container -->

<%- # Javascript at the bottom for fast page loading -%>

<%- # Grab Google CDN's jQuery. fall back to local if necessary -%>
<%= javascript_include_tag '//ajax.googleapis.com/ajax/libs/jquery/1.4.3/jquery.js' %>
<%= javascript_tag %{!window.jQuery && document.write(unescape('%3Cscript src="/javascripts/jquery-1.4.3.min.js"%3E%3C/script%3E'))} %>

<%= javascript_include_tag 'plugins' %>
<%= javascript_include_tag :defaults %>

<%- # Fix any <img> or .png_bg background-images -%>
<!--[if lt IE 7 ]>
  <%= javascript_include_tag 'dd_belatedpng.js' %>
  <%= javascript_tag %{DD_belatedPNG.fix('img, .png_bg')} %>
<![endif]-->

<%- # yui profiler and profileviewer - except for production -%>
<%= javascript_include_tag 'profiling/yahoo-profiling.min.js', 'profiling/config.js' unless Rails.env.production? %>

<%- # asynchronous google analytics: mathiasbynens.be/notes/async-analytics-snippet -%>
<%- # change the UA-XXXXX-X to be your site's ID -%>
<%- if Rails.env.production? -%>
<script>
 var _gaq = [['_setAccount', 'UA-XXXXX-X'], ['_trackPageview']];
 (function(d, t) {
  var g = d.createElement(t),
      s = d.getElementsByTagName(t)[0];
  g.async = true;
  g.src = ('https:' == location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
  s.parentNode.insertBefore(g, s);
 })(document, 'script');
</script>
<%- end -%>